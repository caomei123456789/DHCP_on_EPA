module DHCP
(
		input  wire        i_clk,
		input  wire        i_rst_n
);

		reg [7:0]  i_IP1_1;


always @(posedge i_clk,negedge i_rst_n)
begin
	if(!i_rst_n)	
		i_IP1_1 <= 8'b00000000;
	else 
	   i_IP1_1 <= 8'b11111111;
end

endmodule
